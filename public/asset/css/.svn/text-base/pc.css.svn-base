  /* bread crumbs
  ------------------------------------*/
  .breadcrumb {
    list-style: none;
    overflow: hidden;
    background-color: #f3f3f7;
    margin-bottom: 30px;
  }
  .breadcrumb li {
    float: left;
  }
  .breadcrumb li:first-child a {
    padding-left: 10px;
  }
  .breadcrumb li:last-child {
    padding: 10px 0 10px 55px;
    background-color: #969da5;
    color: #fff;
    position: relative;
  }
  .breadcrumb li:last-child:after {
    content: " ";
    display: block;
    width: 0;
    height: 0;
    border-top: 50px solid transparent;
    border-bottom: 50px solid transparent;
    border-left: 30px solid #969da5;
    position: absolute;
    top: 50%;
    margin-top: -50px;
    left: 100%;
    z-index: 2;
  }
  .breadcrumb li a {
    color: white;
    text-decoration: none;
    padding: 10px 0 10px 55px;
    background: #e4e4e4;
    color: #737b82;
    position: relative;
    display: block;
    float: left;
  }
  .breadcrumb li a:hover {
    color: #f3f3f7;
    background: #737b82;
  }
  .breadcrumb li a:hover:after {
    border-left-color: #737b82 !important;
  }
  .breadcrumb li a:after {
    content: " ";
    display: block;
    width: 0;
    height: 0;
    border-top: 50px solid transparent;
    border-bottom: 50px solid transparent;
    border-left: 30px solid #e4e4e4;
    position: absolute;
    top: 50%;
    margin-top: -50px;
    left: 100%;
    z-index: 2;
  }
  .breadcrumb li a:before {
    content: " ";
    display: block;
    width: 0;
    height: 0;
    border-top: 50px solid transparent;
    border-bottom: 50px solid transparent;
    border-left: 30px solid white;
    position: absolute;
    top: 50%;
    margin-top: -50px;
    margin-left: 1px;
    left: 100%;
    z-index: 1;
  }

  .breadcrumbChild {
    background-color: #e4e4e4;
    margin: -29px 0 30px;
  }
  .breadcrumbChild ul {
    *zoom: 1;
  }
  .breadcrumbChild ul:before, .breadcrumbChild ul:after {
    content: "";
    display: table;
  }
  .breadcrumbChild ul:after {
    clear: both;
  }
  .breadcrumbChild ul li {
    float: left;
    position: relative;
  }
  .breadcrumbChild ul li a {
    padding: 3px 10px;
    color: #737b82;
    display: block;
    -moz-transition-property: color, background;
    -o-transition-property: color, background;
    -webkit-transition-property: color, background;
    transition-property: color, background;
    -moz-transition-duration: 0.3s;
    -o-transition-duration: 0.3s;
    -webkit-transition-duration: 0.3s;
    transition-duration: 0.3s;
    -moz-transition-timing-function: ease;
    -o-transition-timing-function: ease;
    -webkit-transition-timing-function: ease;
    transition-timing-function: ease;
  }
  .breadcrumbChild ul li a:hover {
    color: #fff;
